<script>
  document.addEventListener('DOMContentLoaded', function () {
    const hamburger = document.querySelector('.hamburger');
    const mobileNav = document.querySelector('.mobile-nav');
    const bodyOverlay = document.querySelector('.body-overlay');
    const close = document.querySelector('.close');

    hamburger.addEventListener('click', function () {
      mobileNav.classList.add('open');
      document.querySelector('body').classList.add('menu-open');

      var bodyEvListener = bodyOverlay.addEventListener(
        'click',
        function () {
          mobileNav.classList.remove('open');
          document.querySelector('body').classList.remove('menu-open');
        },
        { once: true }
      );
    });

    close.addEventListener('click', function () {
      mobileNav.classList.remove('open');
      document.querySelector('body').classList.remove('menu-open');
      document.removeEventListener('click', bodyEvListener);
    });

    const navItems = document.querySelectorAll('.nav__item.has-children > a');

    navItems.forEach(function (item) {
      const childList = item.nextElementSibling;
      item.addEventListener('click', function (e) {
        e.preventDefault();
        childList.classList.toggle('open');
        item.classList.toggle('open');
      });
    });
  });
</script>

